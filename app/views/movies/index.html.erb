  <div class="panel panel-default">
    <div class="panel-heading">Movies</div>
    <div class="panel-body">

      <% @movies.each do |movie| %>
        <div class="media">
         <!--  <a class="pull-left" href=<%=movie.image_url%>>
          <img class="media-object" src=<%=movie.image_url%>>
          </a> -->
          <div class="media-body">
            <h4 class="media-heading">
              <%= movie.name %>
            </h4>
            <p><%= movie.release_date %></p>
            <p> <%= movie.genres.pluck(:name)*"," %></p>
            <ul class="list-inline list-unstyled">
              <li><span><i class="glyphicon glyphicon-calendar"></i></span></li>
              <li>|</li>
              <span><i class="glyphicon glyphicon-eye-open"></i> 2</span>
              <li>|</li>
              <li>
                <p> Rating =<%=movie.overall_ratings %> </p>

                <span class="stars">4.8618164</span>
                <span class="stars">2.6545344</span>
                <span class="stars">0.5355</span>
                <span class="stars">8</span>
                <!-- <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star-empty"></span> -->
              </li>
            </ul>
          </div>
        </div>
      <%end%>
      <center>
        <nav>
          <ul class="pagination">
            <li class="disabled"><a href="#"><span aria-hidden="true">«</span><span class="sr-only">Previous</span></a></li>
